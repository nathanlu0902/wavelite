<swiper class="index-swiper" autoplay="true" circular="true" indicator-dots="true" interval="5000" duration="1000">
	<block wx:for="{{bnrUrl}}" wx:for-index="index" wx:key="index">
		<swiper-item>
			<image class="bnr-item-img" src="{{item.imgUrl}}"></image>
		</swiper-item>
	</block>
</swiper>
