<van-row class="btn-wrapper" gutter="50">
	<van-col span="8">
		<view class="btn" style="background-color: #828F9B;" bind:tap="toCount">
			<van-icon size="48rpx" name="shop"></van-icon>
			<view>次卡</view>
		</view>
	</van-col>
	<van-col span="8">
		<view class="btn" style="background-color: #C4712E;" bind:tap="toNutri">
			<van-icon size="48rpx" name="search"></van-icon>
			<view>营养素查询</view>
		</view>
	</van-col>
	<van-col span="8">
		<view class="btn" style="background-color: #99A9A0;" bind:tap="toDelivery">
			<van-icon size="48rpx" name="todo-list"></van-icon>
			<view>订阅</view>
		</view>
	</van-col>
</van-row>
